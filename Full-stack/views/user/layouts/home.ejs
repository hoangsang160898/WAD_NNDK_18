<div class="container-home col-12 m-0 p-0 row">
    <div class="col-xl-2 col-0 p-0 m-0 blank-col-container">
    </div>
    <div class="main-container-home col-xl-8 col-12 m-0 p-0">
        <div class="top-5-hot-news row m-0">
            <div class="col-sm-8 m-0 large-hot-news pl-0 pr-0 pr-sm-1 pb-1 pb-sm-0">
                <div class="owl-carousel owl-carousel-slide-top-5-hot-news owl-theme background-hot-news">
                    <div class="item item-custom row m-0 p-0 row" id="slide1">
                        <div class="img-hot-news col-7 col-sm-7 p-0">
                            <a href="/article/<%= fiveArticlesHot[0].slug %>" class=""><img class="w-100" src="<%= fiveArticlesHot[0].coverImage %>" alt=""></a>
                        </div>
                        <div class="description-hot-news col-5 col-sm-5">
                            <a href="/article/<%= fiveArticlesHot[0].slug %>" class="title-hot-news m-0 text-uppercase"><%= fiveArticlesHot[0].title %>
                            </a>
                            <span class="author m-0 pt-1 row"><a class="text-uppercase"
                                    href="/category/<%= fiveArticlesHot[0].categories[fiveArticlesHot[0].categories.length-1].slug %>/1"><%= fiveArticlesHot[0].categories[fiveArticlesHot[0].categories.length-1].title %></a>&nbsp;&nbsp;<p
                                    class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                        id="date-posted"><%= fiveArticlesHot[0].publishedAt %></span></p></span>
                        </div>
                    </div>
                    <div class="item item-custom row m-0 p-0 row" id="slide2">
                        <div class="img-hot-news col-7 col-sm-7 p-0">
                            <a href="/article/<%= fiveArticlesHot[1].slug %>"><img class="w-100" src="<%= fiveArticlesHot[1].coverImage %>" alt=""></a>
                        </div>
                        <div class="description-hot-news col-5 col-sm-5">
                            <a href="/article/<%= fiveArticlesHot[1].slug %>"
                                class="title-hot-news m-0 text-uppercase"><%= fiveArticlesHot[1].title %></a>
                            <span class="author m-0 pt-1 row"><a class="text-uppercase"
                                    href="/category/<%= fiveArticlesHot[1].categories[fiveArticlesHot[1].categories.length-1].slug %>/1"><%= fiveArticlesHot[1].categories[fiveArticlesHot[1].categories.length-1].title %></a>&nbsp;&nbsp;<p
                                    class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                        id="date-posted"><%= fiveArticlesHot[1].publishedAt %></span></p></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 m-0 small-hot-news background-hot-news p-0 row">
                <div class="img-hot-news col-7 col-sm-12 p-0 m-0">
                    <a href="/article/<%= fiveArticlesHot[2].slug %>" class="">
                        <img class="w-100" src="<%= fiveArticlesHot[2].coverImage %>" alt=""> </a>
                </div>
                <div class="description-hot-news col-5 col-sm-12 m-0">
                    <a href="/article/<%= fiveArticlesHot[2].slug %>" class="title-hot-news m-0"><%= fiveArticlesHot[2].title %>
                    </a>
                    <span class="author m-0 pt-1 row p-0"><a class="text-uppercase"
                            href="/category/<%= fiveArticlesHot[2].categories[fiveArticlesHot[2].categories.length-1].slug %>/1"><%= fiveArticlesHot[2].categories[fiveArticlesHot[2].categories.length-1].title %></a>&nbsp;&nbsp;<p
                            class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                id="date-posted"><%= fiveArticlesHot[2].publishedAt %></span></p></span>
                </div>
            </div>
            <div class="col-sm-12 pt-1 space-line"></div>
            <div class="col-sm-4 m-0 small-hot-news background-hot-news p-0 m-0 row">
                <div class="img-hot-news col-7 col-sm-12 p-0">
                    <a href="/article/<%= fiveArticlesHot[3].slug %>" class="">
                        <img class="w-100" src="<%= fiveArticlesHot[3].coverImage %>" alt=""></a>
                </div>
                <div class="description-hot-news col-5 col-sm-12">
                    <a href="/article/<%= fiveArticlesHot[3].slug %>" class="title-hot-news m-0"><%= fiveArticlesHot[3].title %>
                    </a>
                    <span class="author m-0 pt-1 row"><a class="text-uppercase"
                            href="/category/<%= fiveArticlesHot[3].categories[fiveArticlesHot[3].categories.length-1].slug %>/1"><%= fiveArticlesHot[3].categories[fiveArticlesHot[3].categories.length-1].title %></a>&nbsp;&nbsp;<p
                            class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                id="date-posted"><%= fiveArticlesHot[3].publishedAt %></span></p></span>
                </div>
            </div>
            <div class="col-sm-8 m-0 large-hot-news pl-0 pl-sm-1 pt-1 pt-sm-0 row pr-0">
                <div class="description-hot-news col-sm-5 col-5 background-hot-news">
                    <a href="/article/<%= fiveArticlesHot[4].slug %>" class="title-hot-news m-0 text-uppercase m-0"><%= fiveArticlesHot[4].title %></a>
                    <span class="author m-0 pt-1 row"><a class="text-uppercase"
                            href="/category/<%= fiveArticlesHot[4].categories[fiveArticlesHot[4].categories.length-1].slug %>/1"><%= fiveArticlesHot[4].categories[fiveArticlesHot[4].categories.length-1].title %></a>&nbsp;&nbsp;<p
                            class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                id="date-posted"><%= fiveArticlesHot[4].publishedAt %></span></p></span>
                </div>
                <div class="img-hot-news col-sm-7 col-7 p-0">
                    <a href="/article/<%= fiveArticlesHot[4].slug %>" class="">
                        <img class="w-100" src="<%= fiveArticlesHot[4].coverImage %>" alt=""></a>
                </div>
            </div>
        </div>
        <div class="p-0 m-0 blank-row-container"></div>
        <div class="top-10-lastest_most-read_category">
            <div class="content-top-10-lastest_most-read_category row m-0 p-0 justify-content-between">
                <div class="p-0 col-lg-8 col-md-8">
                    <div class="top-10-lastest-news">
                        <div class="title-top-10-lastest-news col-11 col-lg-12">
                            <p class="m-0 text-uppercase font-weight-bold">top lastest</p>
                        </div>
                        <div class="list-top-10-lastest-news">
                            <% tenArticlesLastest.forEach(function(eachArticleLatest){ %>
                            <div class="lastest-news pb-1 pl-2 pr-2 pt-2 row">
                                <div class="img-lastest-news col-5 col-sm-5 col-md-4"> <a href="/article/<%= eachArticleLatest.slug %>"> <img
                                            class="w-100" src="<%= eachArticleLatest.coverImage %>" alt=""></a></div>
                                <div class="description-lastest-news col-7 col-sm-7 pt-0 pl-3 pr-2 pb-0 pt-md-1">
                                    <a href="/article/<%= eachArticleLatest.slug %>"
                                        class="title-lastest-news m-0 font-weight-bold"><%= eachArticleLatest.title %>
                                    </a>
                                    <span class="author m-0 pt-1 row"><a class=""
                                            href="/category/<%= eachArticleLatest.categories[eachArticleLatest.categories.length-1].slug %>/1"><%= eachArticleLatest.categories[eachArticleLatest.categories.length-1].title %></a>&nbsp;&nbsp;<p
                                            class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                                id="date-posted"><%= eachArticleLatest.publishedAt %></span></p></span>
                                </div>
                            </div>
                            <% }) %>
                        </div>
                    </div>
                    <div class="blank-row-container"></div>
                    <div class="top-12-categories-news pr-md-4 pr-lg-0">
                        <div class="list-top-12-categories-news pr-sm-2 pr-2 pr-lg-0 row">
                            <% twelveArticlesCategory.forEach(function(eachArticleCategory){ %>
                             <% if(eachArticleCategory.category && eachArticleCategory.article) { %>
                            <div class="categories-news pb-2 pt-2 pr-2 pl-3 col-6 col-sm-4 ">
                                <div class="title-categories pb-1">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a
                                        class="text-uppercase "
                                        href="/category/<%= eachArticleCategory.category.slug %>/1"><%= eachArticleCategory.category.title %></a> </div>
                                <div class="img-categories-news mb-lg-1">
                                    <a href="/article/<%= eachArticleCategory.article.slug %>">
                                        <img class="w-100" src="<%= eachArticleCategory.article.coverImage %>" alt="">
                                    </a>
                                </div>
                                <div class="description-categories-news">
                                    <a href="/article/<%= eachArticleCategory.article.slug %>" class="title-categories-news m-0 font-weight-bold">
                                        <%= eachArticleCategory.article.title %>
                                    </a>
                                    <span class="author m-0 pt-1 row"><a class=""
                                            href="/category/<%= eachArticleCategory.category.slug %>/1"><%= eachArticleCategory.category.title %></a>&nbsp;&nbsp;<p
                                            class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                                id="date-posted"><%= eachArticleCategory.article.publishedAt %></span></p>
                                    </span>
                                </div>
                            </div>
                            <% } %>

                            <% }) %>
                        </div>
                    </div>
                </div>
                <div class="top-10-most-read-news p-0 col-lg-3 col-md-4">
                    <div class="title-top-10-most-read-news col-11 col-md-12">
                        <p class="m-0 text-uppercase font-weight-bold">top most read</p>
                    </div>
                    <div class="list-top-10-most-read-news p-0 row m-0">
                        <% tenArticlesMostRead.forEach(function(eachArticleMS){ %>
                        <div class="most-read-news pt-2 pb-1 pl-2 pr-2 col-6 col-md-12 m-0 row">
                            <div class="img-most-read-news col-12"><a href="/article/<%= eachArticleMS.slug %>"><img class="w-100"
                                        src="<%= eachArticleMS.coverImage %>" alt=""></a> </div>
                            <div class="description-most-read-news col-12 p-0 p-md-0 pt-md-1">
                                <a href="/article/<%= eachArticleMS.slug %>"
                                    class="title-most-read-news m-0 font-weight-bold"><%= eachArticleMS.title %>
                                </a>
                                <span class="author m-0 p-0 pt-1 row"><a class=""
                                        href="/category/<%= eachArticleMS.categories[eachArticleMS.categories.length-1].slug %>/1"><%= eachArticleMS.categories[eachArticleMS.categories.length-1].title %></a>&nbsp;&nbsp;<p
                                        class="date-posted m-0"><i class="fa fa-clock-o"></i> <span
                                            id="date-posted"><%= eachArticleMS.publishedAt %></span></p></span></div>
                        </div>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-2 col-0 p-0 m-0 blank-col-container"></div>
</div>